
class fish:
    
  def __init__(self, name, fishtype):
    self.name = name
    self.fishtype = fishtype

f1 = fish("marine",'humantype')
f2 = fish('korramenu','wildtype')
f3 = fish ('African glass catfish','Pareutropius debauwi')
f4 = fish ('African lungfish','genus Protopterus')
f5 = fish ('Aholehole','genus Kuhlia and family Kuhliidae')                                                                     
f6 = fish ('Airbreathing catfish','family Clariidae')
f7 = fish ('Airsac catfish','genus Heteropneustes')
f8 = fish ('Alligatorfish','family Agonidae')
f9 = fish ('Amberjack','genus Seriola')
f10 = fish ('American sole','family Achiridae')
f11 = fish ('Anchovy','family Engraulidae')
f12 = fish ('Angler catfish','genus Chaca')
f13 = fish ('Ayu','Plecoglossus altivelis')
f14 = fish ('Anglerfish','order Lophiiformes')
f15 = fish ('Arapaima','genus Arapaima')
f16 = fish ('Armorhead catfish','genus Cranoglanis')
f17 = fish ('Arowana','family Osteoglossidae')
f18 = fish ('Asiatic glassfish','family Ambassidae')
f19 = fish ('Australasian salmon','genus Arripis and family Arripidae')
f20 = fish ('Australian prowfish','family Pataecidae')

print(f1.name,f1.fishtype)
print(f2.name,f2.fishtype)
print(f3.name,f3.fishtype)
print(f4.name,f4.fishtype)
print(f5.name,f5.fishtype)
print(f6.name,f6.fishtype)
